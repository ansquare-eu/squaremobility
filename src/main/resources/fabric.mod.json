{
  "schemaVersion": 1,
  "id": "squaremobility",
  "version": "${version}",
  "name": "Squaremobility",
  "description": "Travel options for people who like low quality mods",
  "authors": [
    "Farpo (SasaElele) : Owner"
  ],
  "contact": {
    "sources": "https://github.com/ansquare-eu/squaremobility"
  },
  "license": "CC0",
  "icon": "assets/squaremobility/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "eu.ansquare.squaremobility.Squaremobility"
    ],
    "fabric-datagen":  ["eu.ansquare.squaremobility.ModDatagen"]
  },
  "mixins": [
    "squaremobility.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}",
    "create": "${create_version}",
    "minecraft": "${minecraft_version}"
  }
}
